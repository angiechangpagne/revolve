{
  "name": "revolve",
  "version": "1.0.0",
  "description": "nodejs login with jwt token",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/.bin/concurrently \"./node_modules/.bin/nodemon\" \"yarn client\"",
    "build": "cd client && npm install && npm build",
    "client": "cd client && npm start",
    "server": "nodemon server",
    "deploy": "npm buid && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm build",
    "prod": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cronjob": "^0.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "nexmo": "^2.6.0",
    "nodemailer": "^6.4.5",
    "nodemon": "^2.0.2",
    "react-places-autocomplete": "^7.2.1",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "router": "^1.3.4",
    "socket.io": "^2.3.0",
    "twilio": "^3.41.1"
   },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
