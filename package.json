{
  "name": "revolve",
  "version": "1.0.0",
  "description": "nodejs login with jwt token",
  "entry": "server.js",
  "scripts": {
    "start": "cd client && react-scripts start",
    "start:prod": "pushstate-server build",
    "build": "react-scripts build",
    "client": "cd client && yarn start",
    "server": "yarn build && nodemon server.js --watch",
    "deploy": "yarn buid && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "react-scripts test --env=jsdom",
    "postinstall": "yarn build",
    "prod": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn== server\" \"yarn client\"",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cronjob": "^0.1.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "nexmo": "^2.6.0",
    "nodemailer": "^6.4.5",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-places-autocomplete": "^7.2.1",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "router": "^1.3.4",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^2.0.2"
  }
}
